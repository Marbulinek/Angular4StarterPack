  "author": "Lukas Caniga <lukas.caniga@gmail.com>",
  "description": "angular5 lightweight starter pack",
  "scripts": {
    "prod-build": "webpack -p --optimize-minimize",
    "build": "npm run build:dev",
    "build-prod": "npm run build:prod",
    "build:dev": "webpack --env=dev --progress --profile --colors",
