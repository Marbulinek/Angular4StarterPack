{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "dist"
  ]
}
 